*{
				padding: 0px;
				margin: 0px;
			}
			.div-body{
				width: 1128px;
				height: 548px;
			}
			.div-head{
				width: 1128px;
				border: 1px solid blue;
				height: 30px;
				font-family: "楷体";
				color: #FFFFFF;
				font-size: 20px;
				line-height: 30px;
				background-color: blue;

			}
			.div-table-body{
				width: 1128px;
				height: 516px;
			}
			table{
				 width: 1128px;
				 border-collapse: collapse;
			}
			tr{
				width: 1128px;
				border: 1px solid #0000FF;
				border-left-color: #FFFFFF;
				border-right-color: #FFFFFF;
			}
			.fenye-div{
				width: 1128px;
				height: 30px;
				border: 1px solid #0000FF;
				border-top-color: #FFFFFF;
				border-left-color: #FFFFFF;
				border-right-color: #FFFFFF;
				margin-top: 10px;
			}
			.div-center{
				line-height: 30px;
				height: 30px;
				float: left;
				margin-left: 400px;
				margin-right:340px ;
			}
			.div-left-footer{
				float: left;
				width: 150px;
				height: 30px;
				
			}
			.div_up{
				line-height: 30px;
				font-size: 10px;
				width: 100px;
				height: 100px;
				border-bottom-left-radius: 5px;
				border-top-left-radius: 5px;
				border: #CECECE solid 1px;
				padding: 5px;
			}
			.div_down{
				line-height: 30px;
				padding: 5px;
				font-size: 10px;
				width: 100px;
				height: 100px;
				border-bottom-right-radius: 5px;
				border-top-right-radius: 5px;
				border: #CECECE solid 1px;
			}