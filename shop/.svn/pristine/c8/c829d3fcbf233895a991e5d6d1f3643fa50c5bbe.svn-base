package com.shop.pojo;

import java.util.List;

public class ShopCategoryPojo {
	private Integer shopCategoryId ;
	private String shopCategoryNick ;
	private List<ShopChildCategoryPojo> shopChildCategoryPojo;
	public Integer getShopCategoryId() {
		return shopCategoryId;
	}
	public void setShopCategoryId(Integer shopCategoryId) {
		this.shopCategoryId = shopCategoryId;
	}
	public String getShopCategoryNick() {
		return shopCategoryNick;
	}
	public void setShopCategoryNick(String shopCategoryNick) {
		this.shopCategoryNick = shopCategoryNick;
	}
	public List<ShopChildCategoryPojo> getShopChildCategoryPojo() {
		return shopChildCategoryPojo;
	}
	public void setShopChildCategoryPojo(
			List<ShopChildCategoryPojo> shopChildCategoryPojo) {
		this.shopChildCategoryPojo = shopChildCategoryPojo;
	}
	@Override
	public String toString() {
		return "ShopCategoryPojo [shopCategoryId=" + shopCategoryId
				+ ", shopCategoryNick=" + shopCategoryNick + "]";
	}
	
	
	
	
}
